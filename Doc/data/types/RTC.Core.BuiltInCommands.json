" onmouseout=\"this.setAttribute('fill', '#1382CE')\"><![CDATA[help]]></text></a></g></svg>","classDiagramHeight":"225","fit":"False","regions":[{"start":"9","end":"49","filepath":"E:\\VSProjects\\RuntimeConsole\\RTC.Core\\BuiltInCommands.cs","filename":"BuiltInCommands.cs","content":"using RTC.Base; \nusing RTC.Core.Extension; \nusing System; \nusing System.Collections.Generic; \nusing System.Linq; \n \nnamespace RTC.Core \n{ \n    internal sealed class BuiltInCommands \n    { \n        [Command(Alias = \"RTC.Help\", Description = \"Shows additional information about a command.\", Usage = \"RTC.Help [command]\")] \n        private static void help(PlainText command) \n        { \n            if (command.Value.IsNullOrWhiteSpace()) \n            { \n                RTConsole.Instance.Log(LogTypes.Error, \"RTC.Help\", \"Usage: help [command]\"); \n                return; \n            } \n \n            var cmdInfo = RTConsole.Instance.GetCommand(command.Value); \n            if (cmdInfo == null) \n            { \n                RTConsole.Instance.LogFormat(LogTypes.Error, \"RTC.Help\", \"{0} is not a valid command.\", command.Value); \n                return; \n            } \n \n            RTConsole.Instance.Log(LogTypes.Log, string.Format(\"'{0}' usage: {1}\", cmdInfo.Alias, cmdInfo.Usage.Or(\"missing\")), cmdInfo.Description.Or(\"Missing description.\"));         \n        } \n \n        [Command(Alias = \"RTC.Commands\", Description = \"Displays all commands.\", Usage = \"RTC.Commands\")] \n        private static void commands() \n        { \n            List<string> result = new List<string>(); \n \n            var cmds = RTConsole.Instance.Commands; \n            foreach(var cmdInfo in cmds) \n            {  \n                string description = string.Format(\"{0}{1}\", cmdInfo.Description, Environment.NewLine); \n                if(!cmdInfo.Usage.IsNullOrWhiteSpace()) \n                { \n                    description = string.Format(\"{0}Usage: {1}\", description, cmdInfo.Usage); \n                } \n                string format = string.Format(\"<color=lime>{0}</color>{1}{2}{3}\", cmdInfo.Alias, Environment.NewLine, description, Environment.NewLine); \n \n                result.Add(format); \n            } \n            RTConsole.Instance.Log(LogTypes.Log, \"Available commands\", string.Join(Environment.NewLine, result.ToArray())); \n        } \n    } \n}"}],"methods":[{"id":"RTC.Core.BuiltInCommands.commands","urlId":"RTCCoreBuiltInCommandscommands","type":"method","accessibility":"private","name":"commands()","region":{"start":"30","end":"48","filename":"BuiltInCommands.cs"},"syntax":"private static void commands()","linkedSyntax":"private static void commands()"},{"id":"RTC.Core.BuiltInCommands.help(RTC.Base.PlainText)","urlId":"RTCCoreBuiltInCommandshelpRTCBasePlainText","type":"method","accessibility":"private","name":"help(PlainText command)","region":{"start":"11","end":"28","filename":"BuiltInCommands.cs"},"syntax":"private static void help(PlainText command)","linkedSyntax":"private static void help([PlainText](#/type/RTC.Base.PlainText/index) command)"}]}}